{
	"$schema": "https://biomejs.dev/schemas/1.6.3/schema.json",
	"organizeImports": {
		"enabled": true
	},
	"files": {
		"ignore": ["**/dist/**", ".vscode/**", "**/package.json"]
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true,
			"suspicious": {
				"noExplicitAny": "off"
			},
			"style": {
				"noNonNullAssertion": "off",
				"noParameterAssign": "off"
			},
			"complexity": {
				"noForEach": "off"
			}
		}
	}
}
